defregistry stanza-ao :
  file "libfive.cpp" :
    flags "-std=c++11 -I/usr/local/include/eigen3"
  file "libfive.stanza" :
    package libfive :
      dependency "${GEN}/libfive.o"
      link Darwin ["${GEN}/libfive.o" "-lfive" "-lc++"]
      link GNU/Linux ["${GEN}/libfive.o" "-lfive" "-lstdc++"]
  file "libfive-gfx.stanza"
  file "libfive-eval.stanza"
  file "libfive-repl.stanza"
  file "eval-libfive-repl.stanza" gen :
    package eval-libfive-repl
  file "sheet.stanza"
